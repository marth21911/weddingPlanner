@model Wedding
<h2>Plan Your Wedding</h2>
<a href="/logout">Logout</a>
<a href="/dashboard">Dashboard</a>


<form action="/wedding/create" method ="post">
    <input type="hidden" name="UserId" value="@ViewBag.UserinSession">
    <div class="form-group">
        <label for="WedOne">I am getting married and my name is:</label>
        <input asp-for="WedOne">
        <span asp-validation-for="WedOne" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="WedTwo">I am getting married to:</label>
        <input asp-for="WedTwo">
        <span asp-validation-for="WedTwo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="WedDate">Im getting married on:</label>
        <input asp-for="WedDate">
        <span asp-validation-for="WedDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="Address">Its happening at:</label>
        <input asp-for="Address">
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <input type="submit" class="btn btn-success">
</form>